    <convert converter="bean" match="bean.Forca" />
    <convert converter="bean" match="dao.ForcaDAO" />
    <convert converter="bean" match="bean.PostoGraduacao" />
    <convert converter="bean" match="dao.PostoGraduacaoDAO" />
    <convert converter="bean" match="bean.EstadoForca" />
    <convert converter="bean" match="dao.EstadoForcaDAO" />
    <convert converter="bean" match="bean.Om" />
    <convert converter="bean" match="dao.OmDAO" />
